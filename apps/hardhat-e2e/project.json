{
  "root": "apps/hardhat-e2e",
  "sourceRoot": "apps/hardhat-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/hardhat-e2e/cypress.json",
        "devServerTarget": "hardhat:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "hardhat:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/hardhat-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["hardhat"]
}
